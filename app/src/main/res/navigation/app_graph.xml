<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_graph"
    app:startDestination="@+id/fragment_Welcome">


    <fragment
        android:id="@+id/shopPageFragment"
        android:name="com.engineering.dokkan.view.shop.ShopPageFragment"
        android:label="fragment_shop_page"
        tools:layout="@layout/fragment_shop_page" >
        <argument
            android:name="shop_id"
            app:argType="string" />
        <deepLink
            android:id="@+id/deepLink"
            app:uri="http://www.dokkan.com/shops/{shop_id}"
             />
        <argument
            android:name="productId"
            app:argType="string" />
    </fragment>


    <fragment
        android:id="@+id/registerFragment"
        android:name="com.engineering.dokkan.view.register.RegisterFragment"
        android:label="RegisterFragment">
        <action
            android:id="@+id/action_registerFragment_to_LoginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_registerFragment_to_fragment_Welcome"
            app:destination="@id/fragment_Welcome" />

    </fragment>
    <fragment
        android:id="@+id/fragment_Welcome"
        android:name="com.engineering.dokkan.view.welcome.Fragment_Welcome"
        android:label="Fragment_Welcome">
        <action
            android:id="@+id/action_fragment_Welcome_to_registerFragment"
            app:destination="@id/registerFragment"
            app:popUpTo="@id/app_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_fragment_Welcome_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/app_graph"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_fragment_Welcome_to_homeFragment"
            app:destination="@id/homeFragment2"
            app:popUpTo="@id/app_graph"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.engineering.dokkan.view.login.LoginFragment"
        android:label="fragment_login2"
        tools:layout="@layout/fragment_login2" >
        <action
            android:id="@+id/action_loginFragment_to_resetPassword"
            app:destination="@id/resetPassword" />
        <action
            android:id="@+id/action_loginFragment_to_register"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment2"
            app:popUpTo="@id/app_graph"
            app:popUpToInclusive="true"
            app:destination="@id/homeFragment2" />
        <action
            android:id="@+id/action_loginFragment_to_fragment_Welcome"
            app:destination="@id/fragment_Welcome" />
    </fragment>
    <fragment
        android:id="@+id/resetPassword"
        android:name="com.engineering.dokkan.view.login.ResetPassword"
        android:label="fragment_reset_password"
        tools:layout="@layout/fragment_reset_password" >
        <action
            android:id="@+id/action_resetPassword_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>



    <fragment
        android:id="@+id/favourite_Fragment"
        android:name="com.engineering.dokkan.view.Favourite.FavouriteFragment"
        android:label="Favourite_Fragment" >
        <action
            android:id="@+id/action_favourite_Fragment_to_item_Fragment"
            app:destination="@id/item_Fragment" />
        <action
            android:id="@+id/action_favourite_Fragment_to_shop_Fragment"
            app:destination="@id/shop_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/item_Fragment"
        android:name="com.engineering.dokkan.view.Favourite.FavoriteItemFragment"
        android:label="item_fragment"
        tools:layout="@layout/item_fragment" />
    <fragment
        android:id="@+id/shop_Fragment"
        android:name="com.engineering.dokkan.view.Favourite.FavoriteShopFragment"
        android:label="shop_fragment"
        tools:layout="@layout/shop_fragment" >
        <argument
            android:name="shop_id"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/empty_CartFragment"
        android:name="com.engineering.dokkan.view.Empty_Cart.Empty_CartFragment"
        android:label="fragment_empty__cart"
        tools:layout="@layout/fragment_empty__cart" >
        <action
            android:id="@+id/action_empty_CartFragment_to_askQuestionFragment"
            app:destination="@id/askQuestionFragment" />
    </fragment>

    <fragment
        android:id="@+id/chatingFragment"
        android:name="com.engineering.dokkan.view.chating.ChatingFragment"
        android:label="ChatingFragment"
        tools:layout="@layout/fragment_chating" />
    <fragment
        android:id="@+id/askQuestionFragment"
        android:name="com.engineering.dokkan.view.questions.AskQuestionFragment"
        android:label="AskQuestionFragment"
        tools:layout="@layout/fragment_askquestion_frag" />
    <fragment
        android:id="@+id/ChatFragement"
        android:name="com.engineering.dokkan.view.chat.ChatFragement"
        android:label="ChatFragement"
        tools:layout="@layout/fragment_chat_fragement" />

    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.engineering.dokkan.view.notifications.NotificationFragment"
        android:label="NotificationFragment"
        tools:layout="@layout/fragment_notification" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.engineering.dokkan.view.profile.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_chatFragement"
            app:destination="@id/chatFragement" />

        <action
            android:id="@+id/action_profileFragment_to_ordersFragment"
            app:destination="@id/allOrdersListFragment" />
        <action
            android:id="@+id/action_profileFragment_to_addressesfragment"
            app:destination="@id/addressesfragment" />
        <action
            android:id="@+id/action_profileFragment_to_fragment_Welcome"
            app:destination="@id/fragment_Welcome" />
    </fragment>



    <fragment
        android:id="@+id/chatFragement"
        android:name="com.engineering.dokkan.view.chat.ChatFragement"
        android:label="ChatFragement" />
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.engineering.dokkan.view.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment2_to_shopPageFragment"
            app:destination="@id/shopPageFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_askQuestionFragment"
            app:destination="@id/askQuestionFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_profileFragment"
            app:destination="@id/profileFragment" />

        <action
            android:id="@+id/action_homeFragment2_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <argument
            android:name="productId"
            app:argType="string" />

    </fragment>



    <fragment
        android:id="@+id/homeFragment"
        android:name="com.engineering.dokkan.view.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.engineering.dokkan.view.cart.CartFragment"
        android:label="fragment_orders"
        tools:layout="@layout/fragment_cart" >

        <action
            android:id="@+id/action_ordersFragment_to_addressesfragment"
            app:destination="@id/addressesfragment" />


    </fragment>

    <fragment
        android:id="@+id/addressesfragment"
        android:name="com.engineering.dokkan.view.address.AddreesesFragment"
        android:label="fragment_addreeses"
        tools:layout="@layout/fragment_addreeses" >


        <action
            android:id="@+id/action_addressesfragment_to_allOrdersFragment"
            app:destination="@id/allOrdersListFragment"
            app:popUpTo="@id/allOrdersListFragment"
            app:popUpToInclusive="true"/>


        <action
            android:id="@+id/action_addressesfragment_to_addAddressFragment"
            app:destination="@id/addAddressFragment" />

    <fragment
        android:id="@+id/productsFragment"
        android:name="com.engineering.dokkan.view.home.ProductsFragment"
        android:label="ProductsFragment" >
        <argument
            android:name="productId"
            app:argType="string" />
    </fragment>

    </fragment>
    <fragment
        android:id="@+id/addAddressFragment"
        android:name="com.engineering.dokkan.view.address.AddAddressFragment"
        android:label="fragment_add_address"
        tools:layout="@layout/fragment_add_address" >

        <action
            android:id="@+id/action_addAddress_to_AddressFragment"
            app:destination="@id/addressesfragment" />

    </fragment>

    <fragment
        android:id="@+id/productDetailsFragment"
        android:name="com.engineering.dokkan.view.productdetails.ProductDetailsFragment"
        android:label="ProductDetailsFragment" >
        <action
            android:id="@+id/action_productDetailsFragment_to_ordersFragment"
            app:destination="@id/cartFragment" />
    </fragment>
    <action
        android:id="@+id/action_global_to_ProductDetails"
        app:destination="@id/productDetailsFragment" />

    <action
        android:id="@+id/action_global_to_loginFragment"
        app:popUpTo="@id/app_graph"
        app:popUpToInclusive="true"
        app:destination="@id/loginFragment" />
    <fragment
        android:id="@+id/allOrdersListFragment"
        android:name="com.engineering.dokkan.view.orders.allOrders.AllOrdersListFragment"
        android:label="AllOrdersListFragment">

        <action
            android:id="@+id/action_orderList_To_OrderDetails"
            app:destination="@id/orderDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderDetailsFragment"
        android:name="com.engineering.dokkan.view.orders.OrderDetailsFragment.OrderDetailsFragment"
        android:label="OrderDetailsFragment" />


</navigation>